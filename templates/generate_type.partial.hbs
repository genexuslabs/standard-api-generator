{{#if generateClass}}

export class {{sanitizeClassName name}} {
{{#each methods}}
  {{> generate_method this}}
{{/each}}
{{#each properties}}
  {{> generate_property this}}
{{/each}}
}
{{else}}

// Type: {{sanitizeClassName name}}
{{#each methods}}
{{> generate_standalone_function this}}
{{/each}}
// end Type
{{/if}}